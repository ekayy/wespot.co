<% provide(:title, 'Customize') %>
<h2>Customize</h2>


<%= form_for @place do |f| %>

  <div class="control-group">
    <%= f.label :name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :name %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :instagram_tag, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :instagram_tag %>
    </div>
  </div>

<div class="control-group">
    <div class="controls">
  <%= f.submit "Customize" %>
  </div>
</div>
<% end %>
